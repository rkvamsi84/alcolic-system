# Render.com Production Environment Variables for User Web App
# These will be overridden by Render's environment variables

NODE_ENV=production

# API Configuration
REACT_APP_API_URL=https://alcolic-backend-api.onrender.com
REACT_APP_SOCKET_URL=https://alcolic-backend-api.onrender.com

# App Configuration
REACT_APP_NAME=Alcolic User App
REACT_APP_VERSION=1.0.0

# Google Maps (if needed)
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Payment Gateway (if needed)
REACT_APP_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# Analytics (if needed)
REACT_APP_GA_TRACKING_ID=your-google-analytics-id

# Feature Flags
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false

# Build Configuration
GENERATE_SOURCEMAP=false
INLINE_RUNTIME_CHUNK=false