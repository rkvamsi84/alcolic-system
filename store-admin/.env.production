# Render.com Production Environment Variables for Store Admin Panel
# These will be overridden by Render's environment variables

NODE_ENV=production

# API Configuration
VITE_API_URL=https://alcolic-backend-api.onrender.com
VITE_SOCKET_URL=https://alcolic-backend-api.onrender.com

# App Configuration
VITE_APP_NAME=Alcolic Store Admin
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Store Administration Panel

# Authentication
VITE_JWT_STORAGE_KEY=alcolic_store_admin_token
VITE_REFRESH_TOKEN_KEY=alcolic_store_refresh_token

# Google Services (if needed)
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
VITE_GOOGLE_CLOUD_VISION_API_KEY=your-google-vision-api-key

# Payment Integration (if needed)
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key

# OpenAI Integration (if needed)
VITE_OPENAI_API_KEY=your-openai-api-key

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME_UPDATES=true

# Build Configuration
VITE_BUILD_TARGET=es2015
VITE_LEGACY_SUPPORT=false